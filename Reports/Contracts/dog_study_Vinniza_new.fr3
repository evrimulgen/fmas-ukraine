<TfrxReport Name="" Version="3.15" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="2047" PreviewOptions.Zoom="1" PrintOptions.Copies="2" PrintOptions.Printer="Default" PrintOptions.Reverse="True" ReportOptions.CreateDate="39975,4288395139" ReportOptions.Description.Text="" ReportOptions.LastChange="42652,826667581" ScriptLanguage="PascalScript" ScriptText.Text="//var d1,m1,y1,d2,m2,y2:Word;&#13;&#10;var&#13;&#10; kol_month : Word;&#13;&#10;&#13;&#10;&#13;&#10;procedure Page1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if &#60;Draft&#62; = 1&#13;&#10;   then&#13;&#10;    Begin&#13;&#10;     Memo106.visible := true;&#13;&#10;     Memo17.visible := true;&#13;&#10;    End&#13;&#10;   else&#13;&#10;    Begin&#13;&#10;     Memo106.visible := false;&#13;&#10;     Memo17.visible := false;&#13;&#10;    End;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure BarCode1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if &#60;PD_DBDataset.&#34;BARCODE&#34;&#62; &#62;0&#13;&#10;   then BarCode1.text := &#60;PD_DBDataset.&#34;BARCODE&#34;&#62;&#13;&#10;   else BarCode1.visible := false;&#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;procedure Memo42OnBeforePrint(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  d, m, y : Double;&#13;&#10;begin&#13;&#10;  memo42.text := '';&#13;&#10;&#13;&#10;  y := kol_month / 10;&#13;&#10;  m := kol_month - (y * 10);&#13;&#10;&#13;&#10;  if (y = 0)  and (m &#60;&#62; 0) then memo42.text := '1';&#13;&#10;  if (y &#60;&#62; 0) and (m &#60;&#62; 0) then memo42.text := FloatToStr(y) + ',' + FloatToStr(m);&#13;&#10;  if (y &#60;&#62; 0) and (m = 0)  then memo42.text := FloatToStr(y);&#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;procedure Memo194OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;if &#60;PD_DBDataset.&#34;JUR_NAZV_PREDPR&#34;&#62; = '' then&#13;&#10;   Begin&#13;&#10;      Memo49.visible := false;&#13;&#10;      Memo194.visible := false;&#13;&#10;      Memo61.visible := false;